package Ships;
import java.awt.Color;
import java.awt.Graphics;

/**
 * 
 */

/**
 * @author Emmett Deen
 * @version 1.0
 * @date May 17, 2014
 */
public class Scout extends AttackShip{

	
	public Scout(int x, int y, Color t) {
		super(x, y,25,25, t);
		power = 1;
		speed = 5;
		health = 100;
		name = "Scout";
	}
	
	public void paintComponent(Graphics g){
		g.setColor(Color.white);
		g.fillOval(0,0,25,25);
	}
	

	
	
}
